#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_ts-node-dev@1.0.0-pre.43@ts-node-dev/bin/ts-node-dev" "$@"
  ret=$?
else 
  node  "$basedir/../_ts-node-dev@1.0.0-pre.43@ts-node-dev/bin/ts-node-dev" "$@"
  ret=$?
fi
exit $ret
